{
  "name": "mongoose-populate",
  "version": "3.0.2",
  "description": "Mini population hook for Mongoose so you don't need to keep rewriting this.populate and next.",
  "homepage": "https://github.com/dacioromero/mongoose-populate#readme",
  "bugs": "https://github.com/dacioromero/mongoose-populate/issues",
  "repository": "dacioromero/mongoose-populate",
  "license": "MIT",
  "author": "Dacio Romero <dacioromero@gmail.com> (dacio.dev)",
  "main": "dist/index.js",
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "test": "jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && jest",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.ts": "eslint --cache --fix",
    "*.{md,json}": "prettier --write"
  },
  "prettier": "@dacio/prettier-config",
  "eslintConfig": {
    "parserOptions": {
      "project": "./tsconfig.json"
    },
    "extends": "@dacio/eslint-config-typescript"
  },
  "eslintIgnore": [
    "node_modules/",
    "dist/"
  ],
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node"
  },
  "resolutions": {
    "**/mkdirp/minimist": "^0.2.1"
  },
  "dependencies": {
    "@types/mongoose": "^5.7.6"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@dacio/eslint-config": "^0.2.4",
    "@dacio/eslint-config-typescript": "^0.2.5",
    "@dacio/prettier-config": "^0.2.5",
    "@types/jest": "^25.1.4",
    "@typescript-eslint/eslint-plugin": "^2.23.0",
    "@typescript-eslint/parser": "^2.26.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "lint-staged": "^10.0.8",
    "prettier": "^1.19.1",
    "ts-jest": "^25.2.1",
    "typescript": "^3.8.3"
  }
}
